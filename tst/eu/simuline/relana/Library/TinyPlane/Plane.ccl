package TinyPlane;

class Plane extends Component {

  effects 
    output B sLanding     (&(flap1,flap2));
    output B sRollControl (|(flap1,flap3));

    B flap1 (power1);
    B flap2 (|(power1,power2));
    B flap3 (power2);

    B power1 {UNDET:0.002};
    B power2 (f'(sLanding));

  maps
    f : B --> B {
      id: {UNDET}     //{UNDET} |--> UNDET
    };

components
	// no subcomponents 
	//Plane2 subplane;// for tests only
} // class Plane
