package TinyPlane;

class Plane2 {

  effects 
    output B sLanding     (h,(&(flap1,flap2)));
    output B sRollControl (h,(|(flap1,flap3)));

    BoolFlapState flap1 (g'(power1));
    BoolFlapState flap2 (g'(|(power1,power2)));//(|(g,(power1),g,(power2)));
    BoolFlapState flap3 (g'(power2));

    BoolPowerState power1 { replace UNDET {Power:0.002}};
    BoolPowerState power2 (f'(sLanding));

  maps
    f : BoolPowerState --> B {
      {Power} |--> UNDET
    };
    g : BoolFlapState --> BoolPowerState {
      {Moves} |--> Power
    };
    h : BoolFlapState --> B {
      {Moves} |--> UNDET
    };

  components
	// no subcomponents 
} // class Plane2
